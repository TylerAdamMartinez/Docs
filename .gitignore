# Ignore Hugo generated files
/public/
/resources/

# Ignore Hugo config file (config.toml or config.yaml)
/config.toml
/config.yaml

# Ignore Hugo cache and temporary files
/resources/_gen/
/resources/_template_cache/
/resources/_gen-assets/

# Ignore Hugo cache for development server
/resources/_gen/data/
/resources/_gen/images/
/resources/_gen/page/
/resources/_gen/post/
/resources/_gen/sitemap/
/resources/_gen/taxonomy/

# Ignore Hugo rendered files
/resources/_gen/assets/
/resources/_gen/images/

# Ignore Hugo files that can be generated locally
/themes/*/layouts/
/themes/*/static/

# Ignore Hugo .DS_Store files (macOS specific)
.DS_Store

# Ignore the public directory if it's generated locally
/public/

# Ignore Hugo's automatically generated .gitignore file
.gitignore

# Ignore OS-specific or editor-specific files
# For example, ignore files generated by Visual Studio Code
.vscode/
